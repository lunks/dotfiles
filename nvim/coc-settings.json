{
  "tslint.packageManager": "yarn",
  "coc.preferences.formatOnSaveFiletypes": ["typescript.jsx", "typescript"],
  "typescript.implementationsCodeLens.enable": false,
  "solargraph.hover": true,
  "solargraph.autoformat": true,
  "solargraph.diagnostics": true,
  "solargraph.useBundler": true,
  "solargraph.formatting": true,
  "solargraph.completion": true,
  "solargraph.rename": true,
  "solargraph.references": true,
  "solargraph.symbols": true,
  "solargraph.checkGemVersion": false,
  "javascript.referencesCodeLens.enable": false,
  "flow.enable": false,
  "flow.useNPMPackagedFlow": true,
  "javascript.validate.enable": false,
  "typescript.referencesCodeLens.enable": false,
  "tslint.jsEnable": false,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "eslint.enable": true,
  "prettier.bracketSpacing": true,
  "prettier.enable": true,
  "eslint.autoFixOnSave": true,
  "codeLens.enable": true,
  "tslint.autoFixOnSave": false,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete"
}
